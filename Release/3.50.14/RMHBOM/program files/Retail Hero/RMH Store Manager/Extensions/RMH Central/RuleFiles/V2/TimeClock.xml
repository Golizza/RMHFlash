				<Field Name="TimeIn" Type="DateTime" IsMandatory="true" DefaultValue="getdate()"/>
				<Field Name="TimeOut" Type="DateTime" />
				<Field Name="BatchNumber" Type="Int" IsMandatory="true" DefaultValue="0"/>
