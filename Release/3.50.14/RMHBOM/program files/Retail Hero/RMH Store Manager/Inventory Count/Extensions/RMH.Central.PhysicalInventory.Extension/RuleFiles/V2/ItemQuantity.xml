<?xml version="1.0" encoding="utf-8" ?>
<BusinessObject ModuleName="RMHC Physical Inventory Extension" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Prerequisites />
  <Jobs>
    <Job IsMandatory="true" Name="ItemDynamic" Table="ItemDynamic">
      <LinkedJobs />
    </Job>
  </Jobs>
  <JobTemplates>
    <JobTemplate Name="ItemDynamic" Table="ItemDynamic">
      <ForeignFields>
        <ForeignField Name="ItemID" Type="UniqueIdentifier" ReferenceTable="Item" ReferenceColumn="SyncGuid" ReferenceReturn="ID" IsSyncKey ="true" />
      </ForeignFields>
      <Fields>
        <Field Name="Quantity" Type="Float" IsMandatory="true" DefaultValue="0" />
        <Field Name="SnapShotQuantity" Type="Float" />
		<Field Name="QuantityCommitted" Type="Float" IsMandatory="true" DefaultValue="0" />
	  </Fields>
    </JobTemplate>
  </JobTemplates>
</BusinessObject>

